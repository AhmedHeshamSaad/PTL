#define base_dir .
#define num 0.8
#define opt1 1
#define opt2 2
#define num11 1.1
#define num12 1.2
#define num21 2.1
#define num22 2.2
NavierStokes
{
    WallModel
    {
        #define wm_dir WallModelOutput
        useWallModel                = true
        outputWallModelData         = true
        wallModelOutputDataInterval = 13
        forcingOffset               = 4
        errorTolerance              = 3.4e-4
        rayDim                      = 30
        SampleXLocations            = [1.2, 234,42,0.19, 12.3, 2.4]
        outputDir                   = $(base_dir)/$(wm_dir)
        Momentum
        {
        	#define something 3
            equation = ODE
            underRelaxation = $(num$(opt1)$(opt2))$(something)
        }
        Turbulence
        {
            equation = ODE
            underRelaxation = $(num$(opt1)1)
        }
        Energy
        {
            equation = ODE
            underRelaxation = $(num$(opt2)2)
        }
    }
}
